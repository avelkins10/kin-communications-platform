{
  "name": "kin-communications-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx server.js",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "db:generate": "npm run prisma:generate",
    "db:migrate": "npm run prisma:migrate",
    "db:seed": "npm run prisma:seed",
    "db:studio": "npm run prisma:studio",
    "postinstall": "prisma generate",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --config vitest.config.ts --grep \"@integration\"",
    "test:seed": "ts-node tests/seed.ts",
    "test:all": "pnpm test && pnpm test:integration && pnpm e2e",
    "sync:taskrouter": "tsx scripts/sync-taskrouter.ts",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:preview": "playwright test --config playwright.preview.config.ts",
    "e2e:accessibility": "playwright test --config playwright.accessibility.config.ts",
    "e2e:performance": "playwright test --config playwright.performance.config.ts",
    "e2e:visual": "playwright test --config playwright.visual.config.ts",
    "e2e:qa": "playwright test --config playwright.qa.config.ts",
    "e2e:qa:headed": "playwright test --config playwright.qa.config.ts --headed",
    "e2e:qa:ui": "playwright test --config playwright.qa.config.ts --ui",
    "e2e:qa:debug": "playwright test --config playwright.qa.config.ts --debug",
    "e2e:qa:chromium": "playwright test --config playwright.qa.config.ts --project=chromium",
    "e2e:qa:firefox": "playwright test --config playwright.qa.config.ts --project=firefox",
    "e2e:qa:webkit": "playwright test --config playwright.qa.config.ts --project=webkit",
    "e2e:qa:mobile": "playwright test --config playwright.qa.config.ts --project='Mobile Chrome' --project='Mobile Safari'",
    "e2e:qa:cross-browser": "playwright test --config playwright.qa.config.ts --project=chromium --project=firefox --project=webkit",
    "tunnel": "ngrok http --domain=unnoticeably-hornlike-lilli.ngrok-free.dev 3001",
    "webhook:tunnel": "./scripts/ngrok.sh",
    "test:setup": "./scripts/test-setup.sh",
    "test:cleanup": "./scripts/test-setup.sh --cleanup",
    "test:verify": "./scripts/test-setup.sh --verify",
    "dev:test": "NODE_ENV=test node server.js",
    "qa:setup": "chmod +x scripts/qa-testing-setup.sh && ./scripts/qa-testing-setup.sh",
    "qa:run": "chmod +x scripts/qa-testing-runner.sh && ./scripts/qa-testing-runner.sh",
    "qa:cleanup": "chmod +x scripts/qa-testing-cleanup.sh && ./scripts/qa-testing-cleanup.sh",
    "test:qa": "npm run qa:run -- --suite all",
    "test:qa:comprehensive": "npm run qa:run -- --suite comprehensive-qa",
    "test:qa:voice": "npm run qa:run -- --suite voice-calling",
    "test:qa:sms": "npm run qa:run -- --suite sms-messaging",
    "test:qa:voicemail": "npm run qa:run -- --suite voicemail",
    "test:qa:quickbase": "npm run qa:run -- --suite quickbase",
    "test:qa:taskrouter": "npm run qa:run -- --suite taskrouter",
    "test:qa:admin": "npm run qa:run -- --suite admin-panel",
    "test:qa:realtime": "npm run qa:run -- --suite realtime",
    "test:performance": "npm run qa:run -- --suite performance",
    "test:security": "npm run qa:run -- --suite security",
    "test:cross-browser": "npm run qa:run -- --suite cross-browser",
    "test:mobile": "npm run qa:run -- --suite mobile",
    "db:setup:test": "NODE_ENV=test prisma migrate deploy && NODE_ENV=test prisma db seed",
    "db:seed:test": "NODE_ENV=test prisma db seed",
    "validate:platform": "playwright test tests/validation/",
    "validate:ui": "playwright test tests/validation/ui-components.spec.ts",
    "validate:api": "playwright test tests/validation/api-connectivity.spec.ts",
    "validate:socket": "playwright test tests/validation/socket-realtime.spec.ts",
    "validate:auth": "playwright test tests/validation/authentication-flow.spec.ts",
    "validate:readiness": "playwright test tests/validation/platform-readiness.spec.ts",
    "validate:setup": "chmod +x scripts/pre-testing-validation.sh && ./scripts/pre-testing-validation.sh",
    "validate:report": "npm run validate:platform && npm run validate:setup",
    "validate:all": "npm run validate:setup && npm run validate:platform"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.19.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/nextjs": "^10.15.0",
    "@twilio/voice-sdk": "^2.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.0",
    "graphql": "^16.11.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.22",
    "lucide-react": "^0.460.0",
    "next": "^14.2.15",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "p-limit": "^7.1.1",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "sonner": "^1.4.0",
    "twilio": "^5",
    "twilio-taskrouter": "^2.0.0",
    "winston": "^3.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.11.30",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.23",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-vitest": "^0.2.5",
    "jsdom": "^23.0.1",
    "lighthouse": "^11.4.0",
    "lighthouse-ci": "^1.13.1",
    "postcss": "^8.4.47",
    "prisma": "^5.19.1",
    "supertest": "^6.3.3",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.20.5",
    "typescript": "^5.6.2",
    "vitest": "^2.0.0",
    "vitest-mock-extended": "^1.3.1"
  }
}
